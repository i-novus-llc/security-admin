<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-3.0">

    <operations>
        <operation id="create">
            <invocation>
                <java method="create" class="net.n2oapp.security.admin.api.service.BankService">
                    <spring/>
                    <arguments>
                        <argument name="bankEntity" class="net.n2oapp.security.admin.api.model.bank.BankCreateForm" type="entity"/>
                    </arguments>
                </java>
            </invocation>
            <in-parameters>
                <param id="fullName" mapping="fullName" domain="string" />
                <param id="shortName" mapping="shortName" domain="string" />
                <param id="regNum" mapping="regNum" domain="string"/>
                <!--<param name="regDt" mapping="regDt" domain="localdate"/>-->
                <param id="ogrn" mapping="ogrn"/>
                <param id="bik" mapping="bik" domain="string"/>
                <param id="inn" mapping="inn"/>
                <param id="kpp" mapping="kpp" domain="string"/>
                <param id="legalAddress" mapping="legalAddress" domain="string" />
                <param id="actualAddress" mapping="actualAddress" domain="string"/>
                <param id="isCoincidesAddress" mapping="isCoincidesAddress" domain="boolean"/>
                <param id="parent.id" mapping="parentId" domain="string"/>
            </in-parameters>
            <out-parameters>
                <param id="id" mapping="id"/>
            </out-parameters>
        </operation>


        <operation id="update">
            <invocation>
                <java method="update" class="net.n2oapp.security.admin.api.service.BankService">
                    <spring/>
                    <arguments>
                        <argument name="bank" class="net.n2oapp.security.admin.api.model.bank.BankUpdateForm"/>
                    </arguments>
                </java>
            </invocation>
            <in-parameters>
                <param id="id" mapping="id" domain="string"/>
                <param id="fullName" mapping="fullName" domain="string" />
                <param id="shortName" mapping="shortName" domain="string" />
                <param id="regNum" mapping="regNum" domain="string"/>
                <!--<param name="regDt" mapping="regDt" domain="localdate"/>-->
                <param id="ogrn" mapping="ogrn"/>
                <param id="bik" mapping="bik" domain="string"/>
                <param id="inn" mapping="inn"/>
                <param id="kpp" mapping="kpp" domain="string"/>
                <param id="legalAddress" mapping="legalAddress" domain="string" />
                <param id="actualAddress" mapping="actualAddress" domain="string"/>
                <param id="isCoincidesAddress" mapping="isCoincidesAddress" domain="boolean"/>
                <param id="parent.id" mapping="parentId" domain="string"/>
            </in-parameters>
            <out-parameters>
                <param id="id" mapping="id"/>
            </out-parameters>
        </operation>
    </operations>
</object>





