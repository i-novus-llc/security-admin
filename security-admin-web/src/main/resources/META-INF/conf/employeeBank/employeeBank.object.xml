<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-3.0">

    <operations>
        <operation id="create">
            <invocation>
                <java method="create" class="net.n2oapp.security.admin.api.service.EmployeeBankService">
                    <spring/>
                    <arguments>
                        <argument name="userEntity" class="net.n2oapp.security.admin.api.model.EmployeeBankForm" type="entity"/>
                    </arguments>
                </java>


            </invocation>
            <in-parameters>
                <param id="position" mapping="position" domain="string"/>
                <param id="username" mapping="user.username" domain="string"/>
                <param id="email" mapping="user.email" domain="string"/>
                <param id="surname" mapping="user.surname" domain="string"/>
                <param id="name" mapping="user.name" domain="string"/>
                <param id="patronymic" mapping="user.patronymic" domain="string"/>
                <param id="password" mapping="user.password" domain="string"/>
                <param id="passwordCheck" mapping="user.passwordCheck" domain="string"/>
                <param id="roles*.id" mapping="user.roles" domain="integer[]"/>
                <param id="bankId" mapping="bank.id" domain="string"/>
            </in-parameters>
            <out-parameters>
                <param id="id" mapping="id"/>
            </out-parameters>
        </operation>
    </operations>

</object>





