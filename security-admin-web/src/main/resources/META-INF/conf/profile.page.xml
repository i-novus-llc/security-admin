<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0" name="Профиль пользователя">
    <form object-id="blank">
        <fields>
            <output-text id="username" label="Логин" default-value="#{username?}"/>
            <output-text id="surname" label="Фамилия" default-value="#{surname?}"/>
            <output-text id="name" label="Имя" default-value="#{name?}"/>
            <output-text id="patronymic" label="Отчество" default-value="#{patronymic?}"/>
            <output-text id="email" label="Электронная почта" default-value="#{email?}"/>
        </fields>
    </form>
</simple-page>
