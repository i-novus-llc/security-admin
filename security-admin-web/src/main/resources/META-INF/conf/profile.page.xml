<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0"
             name="Профиль пользователя">
    <form>
        <fields>
            <input-text id="username" label="Логин" default-value="#{username}" readonly="true"/>
            <input-text id="surname" label="Фамилия" default-value="#{surname}" readonly="true"/>
            <input-text id="name" label="Имя" default-value="#{name}" readonly="true"/>
            <input-text id="patronymic" label="Отчество" default-value="#{patronymic}" readonly="true"/>
            <input-text id="email" label="Электронная почта" default-value="#{email}" readonly="true"/>
        </fields>
    </form>
</simple-page>